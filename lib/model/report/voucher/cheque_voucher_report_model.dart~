class ChequeVoucherReportModel {
  int id;
  int chequesNumber;
  DateTime paymentDate;
  DateTime issueDate;
  int amount;
  String amountWords;
  int accountId;
  int customerId;
  int supplierId;
  int bankToId;
  int cashId;
  int hrid;
  String beneficiaryName;
  int creationUserId;
  int branchId;
  String branchName;
  int bankFromId;
  String bankName;
  bool isPaid;
  bool isRejected;
  int salesManId;

  ChequeVoucherReportModel({
    required this.id,
    required this.chequesNumber,
    required this.paymentDate,
    required this.issueDate,
    required this.amount,
    required this.amountWords,
    required this.accountId,
    required this.customerId,
    required this.supplierId,
    required this.bankToId,
    required this.cashId,
    required this.hrid,
    required this.beneficiaryName,
    required this.creationUserId,
    required this.branchId,
    required this.branchName,
    required this.bankFromId,
    required this.bankName,
    required this.isPaid,
    required this.isRejected,
    required this.salesManId,
  });

  factory ChequeVoucherReportModel.fromJson(Map<String, dynamic> json) =>
      ChequeVoucherReportModel(
        id: json["ID"],
        chequesNumber: json["ChequesNumber"],
        paymentDate: DateTime.parse(json["PaymentDate"]),
        issueDate: DateTime.parse(json["IssueDate"]),
        amount: json["Amount"],
        amountWords: json["AmountWords"],
        accountId: json["AccountID"],
        customerId: json["CustomerID"],
        supplierId: json["SupplierID"],
        bankToId: json["BankToID"],
        cashId: json["CashID"],
        hrid: json["HRID"],
        beneficiaryName: json["BeneficiaryName"],
        creationUserId: json["CreationUserID"],
        branchId: json["BranchID"],
        branchName: json["BranchName"],
        bankFromId: json["BankFromID"],
        bankName: json["BankName"],
        isPaid: json["IsPaid"],
        isRejected: json["IsRejected"],
        salesManId: json["SalesManID"],
      );

  Map<String, dynamic> toJson() => {
        "ID": id,
        "ChequesNumber": chequesNumber,
        "PaymentDate": paymentDate.toIso8601String(),
        "IssueDate": issueDate.toIso8601String(),
        "Amount": amount,
        "AmountWords": amountWords,
        "AccountID": accountId,
        "CustomerID": customerId,
        "SupplierID": supplierId,
        "BankToID": bankToId,
        "CashID": cashId,
        "HRID": hrid,
        "BeneficiaryName": beneficiaryName,
        "CreationUserID": creationUserId,
        "BranchID": branchId,
        "BranchName": branchName,
        "BankFromID": bankFromId,
        "BankName": bankName,
        "IsPaid": isPaid,
        "IsRejected": isRejected,
        "SalesManID": salesManId,
      };
}
